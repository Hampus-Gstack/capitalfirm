# Google OAuth Configuration
# Get these from https://console.cloud.google.com/apis/credentials
GOOGLE_CLIENT_ID=your_google_client_id_here
GOOGLE_CLIENT_SECRET=your_google_client_secret_here

# NextAuth Configuration
NEXTAUTH_URL=https://capitalfirm-vert.vercel.app
NEXTAUTH_SECRET=your_nextauth_secret_here

# Database Configuration
# For development: postgresql://username:password@localhost:5432/capitalfirm
# For production: Use a managed database service like Supabase, PlanetScale, or Railway
DATABASE_URL="postgresql://username:password@localhost:5432/capitalfirm"

# Vercel Blob Storage Configuration
# Get these from your Vercel project settings
BLOB_READ_WRITE_TOKEN=your_vercel_blob_token_here

# Vercel Edge Config Configuration
# Get this from your Vercel project settings after creating Edge Config
EDGE_CONFIG=your_edge_config_url_here

# Cloudinary Configuration (Free alternative for images/videos)
# Get these from https://cloudinary.com/console
CLOUDINARY_CLOUD_NAME=your_cloud_name_here
CLOUDINARY_API_KEY=your_api_key_here
CLOUDINARY_API_SECRET=your_api_secret_here 